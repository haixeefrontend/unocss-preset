{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "allowJs": true,
    "checkJs": true,
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": true,
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": false,
    "outDir": "dist",
    "declaration": true,
    "declarationDir": "dist",
    "emitDeclarationOnly": true,
    "paths": {
      "unocss-preset-haixee": [
        "./src"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "vue"
  },
  "include": [
    "docs/**/*",
    "src/**/*",
    "tests/**/*",
    "vite.docs.config.ts",
    "vite.config.ts"
  ]
}
